<div class="container">
  <div class="row nav-bar">
    <div class="col-sm-5 col-xs-10">
      <div id="logo-container" style="display:table-cell">
        <a href="/">
          <img src="/photos/<%= conf.logourl %>" width="95" height="60" alt="<%= conf.logoalttext %>"/>
        </a>
        <span style="display:inline-block; vertical-align:bottom; padding-bottom:2px;">ian<strong>cullimore</strong>design</span>
        
      </div>
    </div>
    <div class="col-sm-7 hidden-xs">
      <div id="top-menu" class="nav-menu-container">
        <ul class="nav-menu ">
          <% menuitems.forEach(function(menuitem){ %>
            <% include ./menuitem %>
          <%})%>
        </ul>
      </div>
    </div>
    
    <div id="mobileMenuContainer" class=" col-xs-2 hidden-md hidden-sm hidden-lg mobileMenuContainer">
      <div id="mobileMenu">
        <button id="mobileMenuButton" class="btn btn-default">
          <i class="fa fa-bars"></i>
        </button>
        <nav id="mobileDropDown">
          <% menuitems.forEach(function(menuitem){ %>
            <% include ./mobilemenuitem %>
          <% }) %>
        </nav>
      </div>
    </div>
  </div>
</div>